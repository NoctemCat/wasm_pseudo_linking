{
  "name": "wasm_pseudo_linking_web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devb": "yarn wasmb && yarn dev",
    "devr": "yarn wasmr && yarn dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "wasmb": "cd .. && ./build.sh && cd web/",
    "wasmr": "cd .. && ./rebuild.sh && cd web/"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
