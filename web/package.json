    "@vueuse/core": "^14.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-i18n": "11.1.12",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.10.1",
    "@unhead/addons": "^2.0.19",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
